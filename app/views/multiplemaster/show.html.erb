<h1>Multiplemaster</h1>
<h3>Multiple segments in Accordian Note</h3>


<!-- Main Accordian within a Fieldset -->

	<fieldset id="fieldsetNote1">
		<div id="heading1"></div>
		<ul id="noteList">
		  <li>ITEMVALUE1</li>
          <li>  
                   
              <div class="FloatLt"> 
                  <textarea id="taItemValue1" name="taItemValue1"></textarea>
              </div>
              
              <div class="FloatLt">
                     <input type="button" id ="bItemValueToggle1" value="Toggle">
                     <input type="button" id ="bItemValue1Reset" value="Reset">
                     <input type="button" id ="bItemValue1Hist" value="Hist">
                     <br /><br />
                     <input type="button" id ="bItemValue1Dx" value="Dx:From'update'"> 
                     <br /> 
                     <input type="button" id ="bItemValue1DxYrNote" value="Dx:FromPsychYrNote"> 
                     <br /> 
                     <input type="button" id ="bItemValue1DxMsList" value="Dx:FromMedSpecList">
                     <br /> 
                     <input type="button" id ="bItemValue1Meds" value="Current Meds">
                     <div id="picture"></div>
                     <input type="text" id="dateDOA">
              </div>           
              <div class="clearLtONLY"></div><!---?Needed to stop jitter, or floats behaving badly with toggling-->
        	</li>
       <li>Level2</li>
          <li>  
                   
              <div class="FloatLt"> 
                  <textarea id="taItemValue2" name="taItemValue2"></textarea>
              </div>
              
              <div class="FloatLt">
                     <input type="button" id ="bItemValueToggle2" value="Toggle">
                     <input type="button" id ="bItemValue2Reset" value="Reset">
                     <input type="button" id ="bItemValue2Hist" value="Hist">
                     <br /><br />
